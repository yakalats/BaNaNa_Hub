character = game:GetService("Players").LocalPlayer.Character
pointLight = Instance.new("PointLight")
local Onf = false

Lig.MouseButton1Click:Connect(function()
	pointLight.Parent = character.HumanoidRootPart
	if Onf == false then
		pointLight.Enabled = true
		Onf = true
		Lig.Text = "Light: On"
	else
		pointLight.Enabled = false
		Onf = false
		Lig.Text = "Light: Off"
	end
end)

Grav.MouseButton1Click:Connect(function()
	if workspace.Gravity > 0 then
		workspace.Gravity = 0
		Grav.Text = "Gravity: On"

	else 
		workspace.Gravity = 196.2
		Grav.Text = "Gravity: Off"
	end
end)

local xrayOn = false

Xray.MouseButton1Click:Connect(function(inp, processed)

	xrayOn = not xrayOn

	for i, descendant in pairs(workspace:GetDescendants()) do

		if descendant:IsA("BasePart") then

			if xrayOn then

				if not descendant:FindFirstChild("OriginalTransparency") then

					local originalTransparency = Instance.new("NumberValue")
					originalTransparency.Name = "OriginalTransparency"
					originalTransparency.Value = descendant.Transparency
					originalTransparency.Parent = descendant
				end

				descendant.Transparency = 0.5
				Xray.Text = "XRay: On"

			else
				descendant.Transparency = descendant.OriginalTransparency.Value
				Xray.Text = "XRay: Off"
			end
		end
	end
end)

Noclip.MouseButton1Click:Connect(function()
	local LocalPlayer = game:GetService("Players").LocalPlayer
	local Character = LocalPlayer.Character
	local Humanoid = Character:FindFirstChildOfClass("Humanoid")

	function rm()
		for i,v in pairs(Character:GetDescendants()) do
			if v:IsA("BasePart") then
				if v.Name == "Handle" or v.Name == "Head" then
					if Character.Head:FindFirstChild("OriginalSize") then
						Character.Head.OriginalSize:Destroy()
					end
				else
					for i,cav in pairs(v:GetDescendants()) do
						if cav:IsA("Attachment") then
							if cav:FindFirstChild("OriginalPosition") then
								cav.OriginalPosition:Destroy()  
							end
						end
					end
					v:FindFirstChild("OriginalSize"):Destroy()
					if v:FindFirstChild("AvatarPartScaleType") then
						v:FindFirstChild("AvatarPartScaleType"):Destroy()
					end
				end
			end
		end
	end

	rm()
	wait(0.5)
	Humanoid:FindFirstChild("BodyProportionScale"):Destroy()
	wait(1)

	rm()
	wait(0.5)
	Humanoid:FindFirstChild("BodyHeightScale"):Destroy()
	wait(1)

	rm()
	wait(0.5)
	Humanoid:FindFirstChild("BodyWidthScale"):Destroy()
	wait(1)

	rm()
	wait(0.5)
	Humanoid:FindFirstChild("BodyDepthScale"):Destroy()
	wait(1)

	rm()
	wait(0.5)
	Humanoid:FindFirstChild("HeadScale"):Destroy()
	wait(1)
end)

